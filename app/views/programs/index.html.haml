%div{:style => 'margin: 30px'}
  %h1 Programs

  - @programs.each do |program|
    %h3 
      Program:
      =link_to "#{program.name}", program_path(program.id)
    %p
      %strong Program Info:
      %br
      =program.info
    %p 
      %strong Start Date: 
      =program.start_date
    %p 
      %strong End Date: 
      =program.end_date
    %p
      %strong Status:
      - if program.pending
        Pending
      - else
        Active

  %br

  = link_to 'Back to Dashboard', dashboard_path
