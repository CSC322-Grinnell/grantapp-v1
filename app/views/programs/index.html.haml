%div{:style => 'margin: 30px'}
  %h1 Programs

  - @programs.each do |program|
    %p 
      Program:
      =link_to "#{program.name}", program_path(program.id)
    %p= "Program Info: #{program.info}"
    %p= "Start Date: #{program.start_date}"
    %p= "End Date: #{program.end_date}"
    %p
      Status:
      - if program.pending
        Pending
      - else
        Active

  %br

  = link_to 'Back to Dashboard', dashboard_path
